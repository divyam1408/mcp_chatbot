{
  "servers": [
    {
      "name": "research",
      "description": "ArXiv research paper search and extraction",
      "command": "uv",
      "args": [
        "run",
        "research_server.py"
      ],
      "transport": "stdio",
      "env": null
    },
    {
      "name": "fetch",
      "description": "Fetch contents of given web url.",
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "transport": "stdio",
      "env": null
    },
    {
      "name": "github",
      "description": "Git related operations",
      "transport": "http",
      "url": "https://api.githubcopilot.com/mcp/",
      "headers": {
        "Authorization": "Bearer ${GITHUB_TOKEN}"
      },
      "env": null
    }
  ]
}